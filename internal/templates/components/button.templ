package components

templ Button(label string, attr templ.Attributes) {
	<button type="button" { attr... }>{ label }</button>
}

templ CommonButtonW(label string, attr templ.Attributes) {
	<button type="button" { attr... } class="bg-black text-white rounded-full p-4 border-white border-2 hover:bg-gray-400 ">
		{ label }
	</button>
}
